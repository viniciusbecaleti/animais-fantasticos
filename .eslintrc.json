{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 3],
        "semi": 0,
        "quotes": 0,
        "no-param-reassign": 0,
        "import/extensions": 0,
        "no-console": 0
    }
}
